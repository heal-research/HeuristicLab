    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Field.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\FieldReadOnly.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Indexer.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Interface.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Literal.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Method.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\NameSpace.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Operator.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\OverlayInternal.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\OverlayPrivate.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\OverlayProtected.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\OverlayProtectedInternal.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\OverlayStatic.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\PInvokeMethod.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Property.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\StaticClass.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Struct.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\Template.png" />
    <Resource Include="LanguageFeatures\CodeCompletion\CSharp\CompletionData\Images\VirtualMethod.png" />
  </ItemGroup>
  <PropertyGroup>
    <PreBuildEvent Condition=" '$(OS)' == 'Windows_NT' ">
      set Path=%25Path%25;$(ProjectDir);$(SolutionDir)
      set ProjectDir=$(ProjectDir)
      set SolutionDir=$(SolutionDir)
      set Outdir=$(Outdir)

      call PreBuildEvent.cmd
    </PreBuildEvent>
    <PreBuildEvent Condition=" '$(OS)' != 'Windows_NT' ">
      export ProjectDir=$(ProjectDir)
      export SolutionDir=$(SolutionDir)

      $SolutionDir/PreBuildEvent.sh
    </PreBuildEvent>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.DotNet.UpgradeAssistant.Extensions.Default.Analyzers" Version="0.4.421302">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>