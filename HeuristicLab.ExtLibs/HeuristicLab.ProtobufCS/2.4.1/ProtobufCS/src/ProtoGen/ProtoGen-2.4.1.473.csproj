<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <RootNamespace>Google.ProtocolBuffers.ProtoGen</RootNamespace>
    <AssemblyName>ProtoGen</AssemblyName>
    <AssemblyOriginatorKeyFile>..\..\keys\Google.ProtocolBuffers.snk</AssemblyOriginatorKeyFile>
    <StartupObject>Google.ProtocolBuffers.ProtoGen.ProgramPreprocess</StartupObject>
    <OutDir>bin\$(Platform)\$(Configuration)</OutDir>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\ProtocolBuffers\ProtocolBuffers-2.4.1.473.csproj" />
  </ItemGroup>
  <Target Name="PostBuild_Windows_NT" BeforeTargets="PostBuildEvent" Condition="'$(OS)' == 'Windows_NT'">
    <Exec Command="copy &quot;$(TargetPath)&quot; &quot;$(SolutionDir)&quot;&#xA;copy &quot;$(TargetDir)Google.ProtocolBuffers-2.4.1.473.dll&quot; &quot;$(SolutionDir)&quot;&#xA;copy &quot;$(ProjectDir)..\..\lib\protoc.exe&quot; &quot;$(SolutionDir)&quot;" />
  </Target>
  <Target Name="PostBuild_Not_Windows_NT" AfterTargets="PostBuildEvent" Condition="'$(OS)' != 'Windows_NT'">
    <Exec Command="cp &quot;$(TargetPath)*&quot; &quot;$(SolutionDir)&quot;&#xA;cp &quot;$(TargetDir)Google.ProtocolBuffers-2.4.1.473.dll&quot; &quot;$(SolutionDir)&quot;&#xA;cp &quot;$(ProjectDir)../../lib/protoc.exe&quot; &quot;$(SolutionDir)&quot;" />
  </Target>
  <ItemGroup>
    <PackageReference Include="Microsoft.DotNet.UpgradeAssistant.Extensions.Default.Analyzers" Version="0.4.421302">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>
